{% macro preview_unknown(width=300, height=300, class=None, style=None) -%}
<svg xmlns="http://www.w3.org/2000/svg" version="1.0" width="{{width|int}}" height="{{height|int}}" viewBox="0 0 {{width|int}} {{height|int}}"
    {%- if class %} class="{{class}}" {% endif -%}
    {%- if style %} style="{{style}}" {% endif -%}>
  <rect x="0" y="0" width="{{width|int}}" height="{{height|int}}" fill="#000"/>
  <text x="50%" y="50%" dominate-baseline="middle" text-anchor="middle" fill="#fff">
  ?
  </text>
</svg>
{%- endmacro %}
